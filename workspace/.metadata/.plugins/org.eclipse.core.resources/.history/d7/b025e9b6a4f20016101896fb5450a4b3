package entite;

import java.util.Date;

public class Message  implements IPM{
	String text = "";
	Date date;

	private Message() {
		
	}
	
	public Message(String msgg, Date datee) {
		this.text=msgg;
		this.date=datee; 
	}
	public String getText() {
		return text;
	}

	public void setText(String text) {
		this.text = text;
	}

	public Date getDate() {
		return date;
	}

	public void setDate(Date date) {
		this.date = date;
	}

}
